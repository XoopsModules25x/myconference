RewriteEngine On
RewriteRule	^category-([0-9]+)(-).*(\.html)$	category.php?categories_id=$1
RewriteRule	^category-([0-9]+)(-)([0-9]+)(-).*(\.html)$	category.php?categories_id=$1&start=$3
RewriteRule	^product-([0-9]+)(-).*(\.html)$	product.php?products_id=$1
RewriteRule	^employee-([0-9]+)(-).*(\.html)$	employee.php?employes_id=$1
